    <servlet>
        <servlet-name>PreviewCandidatureServlet</servlet-name>
        <servlet-class>servlet.candidature.PreviewCandidatureServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FinishedCandidatureServlet</servlet-name>
        <servlet-class>servlet.candidature.FinishedCandidatureServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListCandidatureServlet</servlet-name>
        <servlet-class>servlet.candidature.ListCandidatureServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AbscenceServlet</servlet-name>
        <servlet-class>servlet.paie.AbscenceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AjaxProfileServlet</servlet-name>
        <url-pattern>/AjaxProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/ProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListeProfileServlet</servlet-name>
        <url-pattern>/ListeProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteProfileServlet</servlet-name>
        <url-pattern>/DeleteProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PersonalInformationInsertionServlet</servlet-name>
        <url-pattern>/PersonalInformationInsertionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfessionalCareerInsertionServlet</servlet-name>
        <url-pattern>/ProfessionalCareerInsertionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FormationPathInsertionServlet</servlet-name>
        <url-pattern>/FormationPathInsertionServlet</url-pattern>
    </servlet-mapping>
