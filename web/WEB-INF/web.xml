    <servlet>
        <servlet-name>ListeProfileServlet</servlet-name>
        <servlet-class>servlet.profil.ListeProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteProfileServlet</servlet-name>
        <servlet-class>servlet.profil.DeleteProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PersonalInformationInsertionServlet</servlet-name>
        <servlet-class>servlet.candidature.PersonalInformationInsertionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfessionalCareerInsertionServlet</servlet-name>
